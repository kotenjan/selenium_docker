{
	"name": "Selenium Dev Container",
	"build": {
	  "dockerfile": "Dockerfile",
	  "context": ".."
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-toolsai.jupyter-keymap",
				"ms-python.vscode-pylance"
			],
			"settings": {
			  "python.defaultInterpreterPath": "/usr/local/bin/python"
			}
		}
	},
	"remoteUser": "root"
  }